<h1>Messages#index</h1>
<p>Find me in app/views/messages/index.html.erb</p>


    <% @inbox.messages.each do |message| %>
          <span>By <% message.user.first_name %></span>
          <small>At <% message.created_at.strftime("%a %b %e at %l:%M%p") %></small>
        <p><%message.content %></p>
    <% end %>
